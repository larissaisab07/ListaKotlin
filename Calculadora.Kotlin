class Calculadora {
    fun somar (a: Int, b: Int): Int {
        return a + b
    }
    fun subtrair (a: Int, b: Int): Int {
       return a - b 
    }
    fun multiplicar (a: Int, b: Int): Int {
        return a * b
    }
    fun dividir (a: Int, b: Int): Int {
        if (b == 0) {
            throw
   IllegalArgumentException ("Divisão por zero não é permitido.")         
        }
      return a / b 
    }
}

fun main () {
val Calculadora = Calculadora ()

}
